{
  "name": "@brightspace-ui-labs/multi-select",
  "description": "Polymer-based web component for D2L multi select and related components",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUILabs/multi-select.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "localize-behavior.js",
    "multi-select-input-text.js",
    "multi-select-input.js",
    "multi-select-list-item.js",
    "multi-select-list.js"
  ],
  "scripts": {
    "lint": "npm run lint:wc && npm run lint:js",
    "lint:js": "eslint . --ext .js,.html test/**/*.html demo/**/*.html",
    "lint:wc": "polymer lint",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local",
    "test": "npm run lint && npm run test:polymer:local",
    "serve": "polymer serve"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "version": "4.1.7",
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7",
    "@babel/polyfill": "^7",
    "@polymer/iron-test-helpers": "^3",
    "@webcomponents/webcomponentsjs": "^2",
    "eslint": "^7",
    "eslint-config-brightspace": "^0.13",
    "eslint-plugin-html": "^6",
    "eslint-plugin-sort-class-members": "^1",
    "polymer-cli": "^1",
    "wct-browser-legacy": "^1"
  },
  "dependencies": {
    "@brightspace-ui/core": "^1",
    "@polymer/polymer": "^3",
    "d2l-localize-behavior": "BrightspaceUI/localize-behavior#semver:^2",
    "d2l-resize-aware": "BrightspaceUI/resize-aware#semver:^1",
    "d2l-polymer-behaviors": "Brightspace/d2l-polymer-behaviors-ui#semver:^2",
    "d2l-tooltip": "BrightspaceUI/tooltip#semver:^3",
    "d2l-typography": "BrightspaceUI/typography#semver:^7"
  }
}
